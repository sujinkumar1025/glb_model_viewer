{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "ignoreCommand": "echo 'Skipping backend folder during deploy'",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://glb-model-viewer-1.onrender.com/api/$1"
      }
    ]
  }
  